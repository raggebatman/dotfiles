# ------------ #
# binding keys #
# ------------ #

bind Escape abort

bind x delete
bind X kill
bind C-x quit

bind M-h exchangeleft
bind M-j exchangedown
bind M-k exchangeup
bind M-l exhangeright

bind h focusleft
bind j focusdown
bind k focusup
bind l focusright

bind C-v hsplit
bind C-h vsplit

bind space lastmsg

# ------- #
# startup #
# ------- #

exec xterm -e "tmux-init; bash"
